<h1>article edit page</h1>

<form name="_method" action="/articles/<%= @article.id %>" method="POST">
  <input type="hidden" name="_method" value="PATCH">


  <input type="text" name="article[title]" value="<%= @article.title %>" />
  <input type="text" name="article[content]" value="<%= @article.content %>" />

  <input id="Submit" type="submit" value="Update">

</form>
